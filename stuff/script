arr = Array.new(100)

(0..99).each do |i|
    arr[i] = i + 1
    if arr[i] % 3 == 0 && arr[i] % 5 == 0
        p "FizzBuzz, #{arr[i]}"
    elsif arr[i] % 3 == 0
        p "Fizz, #{arr[i]}"
    elsif arr[i] % 5 == 0
        p "Buzz, #{arr[i]}"
    else
        p "#{arr[i]}"
    end
    #p "value = #{arr[i]}"
end
#p arr.to_s